        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.1.0",
        "prettier": "3.1.0",
        "typescript": "^5.2.2"
    },
    "lint-staged": {
        "*.ts": [
            "npm run lint:fix",
